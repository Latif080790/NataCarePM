{
  "name": "natacare-project-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "lint:report": "eslint . --format html --output-file lint-report.html",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint:check",
    "quality:fix": "npm run lint:fix && npm run type-check",
    "pre-commit": "node scripts/pre-commit.js",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "deploy:firestore": "firebase deploy --only firestore:rules",
    "deploy:storage": "firebase deploy --only storage:rules",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:all": "npm run build && firebase deploy",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init",
    "firebase:emulators": "firebase emulators:start"
  },
