# E2E Test Environment Variables
# Copy this file to .env.test and fill in actual values

# Application URL
VITE_APP_URL=http://localhost:4173

# Test User Credentials
# Create a dedicated test user in Firebase for E2E tests
TEST_USER_EMAIL=test@natacare.com
TEST_USER_PASSWORD=Test123!@#
TEST_USER_NAME=Test User

# Optional: Admin Test User (for admin-only features)
TEST_ADMIN_EMAIL=admin@natacare.com
TEST_ADMIN_PASSWORD=Admin123!@#

# Playwright Configuration
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=30000

# Test Data Cleanup
# Set to 'true' to automatically delete test data after each run
CLEANUP_TEST_DATA=false

# Firebase Test Project (optional - use separate project for testing)
# VITE_FIREBASE_API_KEY=...
# VITE_FIREBASE_AUTH_DOMAIN=...
# VITE_FIREBASE_PROJECT_ID=...
